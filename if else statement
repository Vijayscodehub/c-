// if else statement

#include <iostream>
using namespace std;
int main()
{
    int a;
    int b;
    cout<<"Please enter value of a"<<"\n";
    cin>>a;
    cout<<"Please enter value of b"<<"\n";
    cin>>b;
    if (a>b)
    {
        cout<<"a is greater than b"<<endl;
    }
    else
    {
        cout<<"b is greater than a"<<endl;
    }
}
